<div class="container margin-from-navbar dashboard-color">
<br>
<br>
<br>
  <div class="">
    <%# nav start %>
      <nav class="nav nav-tabs">
        <a class="nav-link active link-font" aria-current="page" href="#writings" data-bs-toggle="tab">📑</a>
        <a class="nav-link link-font" href="#newwriting" data-bs-toggle="tab">✍🏻</a>
        <a class="nav-link link-font" aria-current="page" href="#projects" data-bs-toggle="tab">🖼</a>
        <a class="nav-link link-font" href="#newproject" data-bs-toggle="tab">✍🏻</a>
        <a class="nav-link link-font" href="#profile" data-bs-toggle="tab">👱🏻‍♂️</a>
        <a class="nav-link link-font" href="#moodboard" data-bs-toggle="tab">💡</a>
      </nav>
    <%# nav end %>

    <%# content start %>
    <div class="container-60">
      <div class="d-flex justify-content-end pe-2 pt-1 text-end">
      <div>
        <%= link_to  " X sign out", destroy_user_session_path, data: { "turbo-method": :delete }, id: "link-red" %>
      </div>
    </div>
      <div class="tab-content" id="myTabContent">
        <div class="tab-pane fade show active" id="writings" role="tabpanel">
          <%= render  "pages/dashboard/writings_tab" %>
        </div>

        <div class="tab-pane fade show" id="newwriting" role="tabpanel">
          <h2 class="mb-4">New Writing</h2>
          <%= render  "writings/new" %>
        </div>

        <div class="tab-pane fade show" id="projects" role="tabpanel">
          <%= render  "pages/dashboard/projects_tab" %>
        </div>

        <div class="tab-pane fade show" id="newproject" role="tabpanel">
        <h2 class="mb-4">New Project</h2>
          <%= render  "/projects/new" %>
        </div>

        <div class="tab-pane fade show" id="profile" role="tabpanel">
          <%= render  "pages/dashboard/profile_tab" %>
        </div>

         <div class="tab-pane fade show" id="moodboard" role="tabpanel">
          <%= render  "pages/dashboard/moodboard_tab" %>
        </div>
    </div>
    <%# content end %>
  </div>
</div>
